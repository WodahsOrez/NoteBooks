{
    "title": "第一本GitBook",
    "description": "Typora的使用",
    "author": "WodahsOrez",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Home": "http://###"
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "-highlight",
		"prism",                 
		"prism-themes",
        "-livereload",
        "search-plus",
		"splitter",              
		"copy-code-button",      
		"todo",                  
		"anchor-navigation-ex",  
        "anchors",                
		"expandable-chapters",
		"-toggle-chapters"
    ],

    "pluginsConfig": {
       "prism": {
            "css": [
                "prism-themes/themes/prism-atom-dark.css"
            ]
        },
		"theme-default": {
			"showLevel": true
		},
		"anchor-navigation-ex": {
			"multipleH1": false,
			"associatedWithSummary": false,
			"showLevel": true
		}
    }
}